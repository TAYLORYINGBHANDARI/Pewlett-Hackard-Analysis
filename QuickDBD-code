Departments
-
dept_no varchar pk fk - Dept_emp.dept_no
dept_name varcahr

Dept_emp
-
dept_no varchar pk 
emp_no int pk FK >- Salaries.emp_no
from_date date
to_date date

Managers
-
dept_no varchar pk fk - Departments.dept_no
emo_no int pk fk - Employees.emp_no
from_date date
to_date date

Employees
-
emp_no int pk FK >- Titles.emp_no
birth_date date
first_name varcahr
last_name varchar
gender varchar
hire_date date

Salaries
-
emp_no int pk FK >- Employees.emp_no
salary int
from_date date
to_date date

Titles
-
emp_no int pk FK >-< Salaries.emp_no
title varchar
from_date date
to_date date

